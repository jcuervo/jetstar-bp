          <div id="departDetails" class="hidden"></div>
          <div id="dpDate" class="halfw left">Oct<br />2011</div>
          <div id="dpDay" class="halfw left">16</div>
        </div>
      </div>
    </li>
    <li>
      <div class="schedtaps left" id="rd">
        <h4>Return</h4>
        <div>
          <div id="returnDetails" class="hidden"></div>
          <div id="rpDate" class="halfw">Oct<br />2011</div>
          <div id="rpDay" class="halfw">16</div>
        </div>
      </div>
    </li>
  </ul>
</div>
<div id="dps" class="left fullwidth">
  <div id="datepickerD">
  </div>
  <div id="datepickerR" class="hidden">
  </div>
</div>



